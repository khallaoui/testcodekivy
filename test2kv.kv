<W1>:
    name:'W1'
    GridLayout:
        cols:1
        GridLayout:
            rows:4
            Image:
                source:'img3.png'
            Label:
                text: 'LOGING'
                color: 1 , 0 , 0 , 1
                font_size: 22
            Label:
                text: 'PLEASE ENTER PASSWORD:'
                font_hint:(0.1,0.3)

            TextInput:
                id:passw
                text:''
                multiline: False
                size_hint:(0.1,0.3)

        Button:
            text:'LOGIN'
            size_hint:(0.1,0.1)
            on_release:
                app.root.current='W2' if passw.text=='1' else 'Error'
                root.manager.transition.direction='up'

