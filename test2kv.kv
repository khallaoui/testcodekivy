<Mod>:
    name:'Mod'
    GridLayout:
        rows:2
        Label:
            id:LMOD
            text:'ENTREZ LES NOTES DES MODULES DE S1: '
            color: .89 , .97 , .130 , 1
            font_size: 16
        GridLayout:
            cols:2
            rows:5
            Label:
                text:'MODELE: 1'

            TextInput:
                id:M1
                multiline:False
                text:'hn'
            Label:
                text:'MODELE: 2'

            TextInput:
                id:M2
                multiline:False
                text:''
            Label:
                text:'MODELE: 3'

            TextInput:
                multiline:False
                id:M3
                text:''
            Label:
                text:'MODELE: 4'
            TextInput:
                multiline:False
                id:M4
                text:''

            Button:
                text:'RESULTAT:'
                on_release:
                    M11=float(M1.text)
                    M22=float(M2.text)
                    M33=float(M3.text)
                    M44=float(M4.text)
                    mods=[M11,M22,M33,M44]
                    for i in mods:\
                    print(i)
                    root.manager.transition.direction='up'


